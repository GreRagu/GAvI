                                      <mml:mi>l</mml:mi>                                                               <mml:mo>)</mml:mo>                                                            </mml:mrow>                                                         </mml:mrow>                                                      </mml:msub>                                                      <mml:msub>                                                         <mml:mi>&#x003b8;</mml:mi>                                                         <mml:mrow>                                                            <mml:mi>i</mml:mi>                                                            <mml:mo>,</mml:mo>                                                            <mml:mi>j</mml:mi>                                                         </mml:mrow>                                                      </mml:msub>                                                   </mml:mrow>                                                </mml:mstyle>                                             </mml:mrow>                                          </mml:mstyle>                                       </mml:mrow>                                       <mml:mo>)</mml:mo>                                    </mml:mrow>                                 </mml:mrow>                                 <mml:mn>2</mml:mn>                              </mml:msup>                           </mml:mrow>                        </mml:mstyle>                        <mml:mo>+</mml:mo>                        <mml:mi>&#x003bc;</mml:mi>                        <mml:mo>&#x022c5;</mml:mo>                        <mml:mstyle displaystyle="true">                           <mml:munderover>                              <mml:mo>&#x02211;</mml:mo>                              <mml:mrow>                                 <mml:mi>i</mml:mi>                                 <mml:mo>=</mml:mo>                                 <mml:mn