www.w3.org/1998/Math/MathML" id="M51" name="1471-2105-9-S12-S17-i35" overflow="scroll">                     <mml:semantics>                        <mml:mrow>                           <mml:mtable>                              <mml:mtr>                                 <mml:mtd>                                    <mml:mrow>                                       <mml:mstyle mathvariant="bold" mathsize="normal">                                          <mml:mi>z</mml:mi>                                       </mml:mstyle>                                       <mml:mo>=</mml:mo>                                       <mml:mrow>                                          <mml:mo>[</mml:mo>                                          <mml:mrow>                                             <mml:mtable>                                                <mml:mtr>                                                  <mml:mtd>                                                  <mml:mi>&#x003b8;</mml:mi>                                                  </mml:mtd>                                                </mml:mtr>                                                <mml:mtr>                                                  <mml:mtd>                                                  <mml:mrow>                                                  <mml:msub>                                                  <mml:mstyle mathvariant="bold" mathsize="normal">                                                  <mml:mi>x</mml:mi>                                                  </mml:mstyle>                                                  <mml:mn>0</mml:mn>                                                  </mml:msub>                                                  </mml:mrow>                                                  </mml:mtd>                                                </mml:mtr>                                                <mml:mtr>                                                  <mml:mtd>  