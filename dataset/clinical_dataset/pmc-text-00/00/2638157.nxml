mml:mtd>                                       <mml:mtd>                                          <mml:mo>&#x022ee;</mml:mo>                                       </mml:mtd>                                    </mml:mtr>                                    <mml:mtr>                                       <mml:mtd>                                          <mml:mrow>                                             <mml:msub>                                                <mml:mi>g</mml:mi>                                                <mml:mrow>                                                  <mml:mi>q</mml:mi>                                                  <mml:mn>1</mml:mn>                                                </mml:mrow>                                             </mml:msub>                                             <mml:msubsup>                                                <mml:mi>x</mml:mi>                                                <mml:mn>1</mml:mn>                                                <mml:mrow>                                                  <mml:mo>&#x02212;</mml:mo>                                                  <mml:mn>1</mml:mn>                                                </mml:mrow>                                             </mml:msubsup>                                             <mml:msub>                                                <mml:mi>v</mml:mi>                                                <mml:mi>q</mml:mi>                                             </mml:msub>                                          </mml:mrow>                                       </mml:mtd>                                       <mml:mtd>                                          <mml:mrow>                                             <mml:msub>                                                <mml:mi>g</mml:mi>                                                <mml:mrow>                                                  <mml:mi>q</mml:mi>    